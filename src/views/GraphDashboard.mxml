			private function parseDate( str : String ) : Date {
				var matches : Array = str.match(/(\d\d)\/(\d\d)\/(\d\d\d\d)/);
				var d : Date = new Date();
				d.setUTCFullYear(int(matches[3]), int(matches[1]) - 1, int(matches[2]));
				return d;
			}
			
